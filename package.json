{
  "name": "NfaDb",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@cap-js/hana": "^2",
    "@sap-cloud-sdk/http-client": "^4.1.1",
    "@sap-cloud-sdk/resilience": "^4.1.1",
    "@sap/cds": "^9",
    "@sap/hdi-deploy": "^5.5.1",
    "express": "^4",
    "hdb": "^0.19.12",
    "path": "^0.12.7",
    "xlsx": "^0.18.5"
  },
  "engines": {
    "node": ">=20"
  },
  "devDependencies": {
    "@cap-js/cds-types": "^0.13.0",
    "@cap-js/sqlite": "^2",
    "@sap/cds-dk": ">=8",
    "cds-plugin-ui5": "^0.13.0",
    "mbt": "^1.2.29",
    "rimraf": "^5.0.5"
  },
  "scripts": {
    "start": "cds-serve",
    "watch-nfadbapp": "cds watch --open nfadbapp/index.html?sap-ui-xx-viewCache=false --livereload false",
    "undeploy": "cf undeploy NfaDb --delete-services --delete-service-keys --delete-service-brokers",
    "build": "rimraf resources mta_archives && mbt build --mtar archive",
    "deploy": "cf deploy mta_archives/archive.mtar --retries 1"
  },
  "cds": {
    "requires": {
      "destinations": true,
      "TestApi": {
        "kind": "odata",
        "credentials": {
          "destination": "DEMO"
        }
      },
      "NfaAriba": {
        "kind": "odata",
        "credentials": {
          "destination": "NfaAriba"
        }
      }
    },
    "[production]": {
      "requires": {
        "auth": {
          "kind": "xsuaa"
        }
      }
    }
  },
  "workspaces": [
    "app/*"
  ]
}
